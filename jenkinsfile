pipeline {
     agent any
         tools {
    maven 'maven'
  }
     stages{
        stage('Run tests interface') {
            steps{
                    git credentialsId:'gitlab_login' ,url:'https://github.com/RodrigoQA/sicredi-automation-tests-web'
                    sh 'mvn test -Dgroups="add-cliente,excluir-cliente"'
                }
            }

         }
    }
